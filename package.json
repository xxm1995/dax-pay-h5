    "rimraf": "^3.0.2",
    "rollup": "^4.22.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.2",
    "unocss": "^0.58.9",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "vite": "5.2.6",
    "vite-plugin-compression": "^0.5.1",
